<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">

<struts>


	<package name="idv.trans.struts.user"  namespace="/Admin"  extends="struts-default">
		<action name="user_add" class="idv.trans.struts.action.User" method="addForm">
			<result name="SUCCESS">/Template/Admin/user_add.jsp</result>
		</action>
		<action name="user_add_save" class="idv.trans.struts.action.User" method="save">
			<result name="EDIT_ERROR">/Template/Admin/user_edit.jsp</result>
			<result name="UPDATE_SUCCESS">/Template/Admin/user_list.jsp</result>
			<result name="ADD_SUCCESS">/Template/Admin/user_add.jsp</result>
			
			<result name="ADD_ERROR">/Template/Admin/user_add.jsp</result>
		</action>
		<action name="user_search" class="idv.trans.struts.action.User" method="toSearch">
			<result name="SUCCESS">/Template/Admin/user_search.jsp</result>
		</action>
		<action name="user_search_result" class="idv.trans.struts.action.User" method="search">
			<result name="SUCCESS">/Template/Admin/user_list.jsp</result>
		</action>
		<action name="user_edit" class="idv.trans.struts.action.User" method="edit">
			<result name="SUCCESS">/Template/Admin/user_edit.jsp</result>
		</action>
		<action name="user_pwd_change" class="idv.trans.struts.action.User" method="changePwd">
			<result name="SUCCESS">/Template/Admin/user_pwd_change.jsp</result>
		</action>
		
		<action name="user_pwd_save" class="idv.trans.struts.action.User" method="changePwdSave">
			<result name="SUCCESS" >/Template/Admin/user_pwd_change.jsp</result>
			<result name="ERROR" >/Template/Admin/user_pwd_change.jsp</result>
		</action>
		
		
	</package>
</struts>
